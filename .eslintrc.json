{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "strict": [
      "error",
      "global"
    ],
    "no-console": 0
  },
  "globals": {
    "React": false,
    "ReactDOM": false
  }
}
